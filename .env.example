# Server configuration
PORT=3000

# Template directory (optional)
TEMPLATE_DIR=templates

# OpenAI API key for LangChain agent
OPENAI_API_KEY=

# Default temperature for OpenAI LLM (optional, default: 0.7)
OPENAI_TEMPERATURE=0.7

# OpenAI model name (optional, default: gpt-4)
OPENAI_MODEL=gpt-4

# OpenAI API request timeout in milliseconds (optional, default: 60000 / 60 seconds)
OPENAI_TIMEOUT_MS=120000
